{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "all",
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "importOrder": [
    "^[react|next]$",
    "^@mui",
    "<THIRD_PARTY_MODULES>",
    "^Pages",
    "^Components",
    "^Redux",
    "^Clients",
    "^Shared",
    "^[../](.*)(?<!.css)$",
    "^[./](.*)(?<!.css)$",
    "(.*)css$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}
